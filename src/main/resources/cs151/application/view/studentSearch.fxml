<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs151.application.controller.StudentSearchController">
    <children>
        <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="670" prefWidth="800" style="-fx-background-color: #93AEC4;">
            <children>
                <ToolBar blendMode="COLOR_BURN" prefHeight="37.0" prefWidth="632.0">
                    <items>
                        <Button mnemonicParsing="false" onAction="#onHomeButtonClick" text="Home" />
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onBackToProfilePageButtonClick" text="Student Profile" />
                    </items>
                </ToolBar>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="59.0" prefWidth="817.0" text="Search, Edit Profiles, Add/View Comments">
                    <font>
                        <Font name="Avenir Roman" size="40.0" />
                    </font>
                </Label>
                <TextField fx:id="searchEntry" alignment="CENTER" onKeyTyped="#onSearchFieldEnter" prefHeight="26.0" prefWidth="300.0" promptText="Enter Search Text">
                    <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                    </VBox.margin>
                </TextField>
                <TableView fx:id="studentTableView" maxWidth="799.0" onKeyTyped="#onSearchFieldEnter" prefHeight="434.0" prefWidth="760.0">
                    <columns>
                        <TableColumn fx:id="studentNameColumn" editable="false" maxWidth="430.0" minWidth="101.0" prefWidth="101.0" text="Student Name" />
                        <TableColumn fx:id="academicStatusColumn" prefWidth="95.5" text="Academic Status" />
                        <TableColumn fx:id="jobColumn" prefWidth="101.5" text="Job Status" />
                        <TableColumn fx:id="jobDetailsColumn" prefWidth="90.0" text="Job Details" />
                        <TableColumn fx:id="programmingLangColumn" prefWidth="87.0" text="Programming Languages " />
                        <TableColumn fx:id="databasesColumn" prefWidth="69.5" text="Databases" />
                        <TableColumn fx:id="preferredRoleColumn" prefWidth="109.0" text="Preferred Role" />
                        <TableColumn fx:id="flagsColumn" prefWidth="75.0" text="Flags" />
                        <TableColumn fx:id="evaluationColumn" prefWidth="75.0" text="Evaluations" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                    <VBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </VBox.margin>
                </TableView>
                <ButtonBar prefHeight="46.0" prefWidth="800.0">
                    <buttons>
                        <Button fx:id="editButton" layoutX="740.0" layoutY="17.0" mnemonicParsing="false" onAction="#onEditProfileClick" text="Edit" translateX="-20.0" translateY="-10.0" />
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" translateX="-20.0" translateY="-10.0" />
                        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onAddCommentClick" prefWidth="133.0" text="Add/View Comments" translateX="-20.0" translateY="-10.0" />
                    </buttons>
                </ButtonBar>
            </children>
        </VBox>
    </children>
</AnchorPane>
