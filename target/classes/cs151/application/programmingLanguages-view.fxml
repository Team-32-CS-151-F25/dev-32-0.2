<VBox alignment="CENTER" prefHeight="448.0" prefWidth="632.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs151.application.MainController">
   <children>
      <VBox prefHeight="481.0" prefWidth="632.0" style="-fx-background-color: #93AEC4;">
         <children>
            <TextFlow prefHeight="459.0" prefWidth="619.0">
               <children>
                  <ToolBar prefHeight="37.0" prefWidth="632.0">
                    <items>
                      <Button mnemonicParsing="false" text="Home" />
                    </items>
                  </ToolBar>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="50.0" prefWidth="632.0" text="Programming Languages">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER_RIGHT" layoutY="10.0" prefHeight="44.0" prefWidth="208.0" text="Language Names *: " />
                  <TextField fx:id="languageNameInput" prefHeight="26.0" prefWidth="254.0" promptText="Enter a programming language" translateX="11.0" translateY="16.0" />
                  <Button mnemonicParsing="false" onAction="#onAddLanguageClick" prefHeight="26.0" prefWidth="45.0" text="Add" translateX="19.0" translateY="16.0" />
                  <Label alignment="CENTER_RIGHT" prefHeight="44.0" prefWidth="208.0" text="Inputted Languges: " translateX="-1.0" translateY="29.0" />
                  <TextArea fx:id="languageList" editable="false" prefHeight="159.0" prefWidth="430.0" translateX="98.0" translateY="28.0" />
               </children>
            </TextFlow>
         </children>
      </VBox>
   </children>
